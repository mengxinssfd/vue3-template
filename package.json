{
  "name": "vue3-template",
  "version": "1.0.0",
  "description": "vue3项目模板",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/vue3-template.git"
  },
  "author": "DYH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengxinssfd/vue3-template/issues"
  },
  "homepage": "https://github.com/mengxinssfd/vue3-template#readme",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "husky-msg-init": "husky add .husky/pre-commit \"npx lint-staged --allow-empty \"$1\"\"&yarn husky add .husky/commit-msg \"npx commitlint --edit \"$1\"\""
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@vitejs/plugin-vue": "^2.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vue-tsc": "^0.33.9",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "prettier": "^2.6.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "vite-plugin-html": "^3.2.0",
    "less": "^4.1.2",
    "@types/less": "^3.0.3",
    "postcss": "^8.4.12",
    "postcss-px-to-viewport": "^1.1.1",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "autoprefixer": "^10.4.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
